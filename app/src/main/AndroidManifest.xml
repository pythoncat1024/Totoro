<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pythoncat.totoro">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/totoro"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme_C">
        <activity
            android:name=".activity.LauncherActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.historyitem.HistoryItemActivity" />
        <activity android:name=".activity.detail.DetailActivity" />
        <activity android:name=".activity.PersonalActivity"></activity>
    </application>

</manifest>